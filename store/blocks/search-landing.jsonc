{
    "store.custom#landing": {
        "blocks":[
            "image#landing",
            "search-result-layout.customQuery#landing"    
        ]
    },

    "image#landing": {
        "props":{
            "src": "https://appliancetheme.vteximg.com.br/arquivos/cozinha-about-us.png",
            "minWidth": "100%"
        }
    },

    "search-result-layout.customQuery#landing": {
        "children": [
            "search-result-layout.desktop"
        ],
        "props":{
            "querySchema":{
                "orderByField": "OrderByReleaseDateDESC",
                "maxItemsPerPage": 8,
                "hideUnavailableItems": false,
                "facetsBehavior": "static",
                "skusFilter": "ALL"
            }
        }
    },

    "search-result-layout.desktop": {
        "children": [
            "search-content"
        ],
        "props":{
            "hiddenFacets": {
                "specificationFilters": {
                    "hiddenFilters": "Blue Top Retro Camera"
                }
            }
        }
    },

    "search-content": {
        "children":[
            "gallery"
        ]
    }
    
}